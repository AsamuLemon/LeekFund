{
    "manifest_version":2,
    "name":"韭菜工具",
    "version":"1.0.0",
    "description":"一款合格韭菜的基金管理插件",
    "icons":{
        "16":"img/doinb.jpg",
        "48":"img/doinb.jpg",
        "128":"img/doinb.jpg" 
    },
    "background":{
        "page":"background.html"
    },
    "browser_action":{
        "default_icon":"img//doinb.jpg",
        "default_title":"稳住我们能赢",
        "default_popup":"popup.html"
    },
    "content_scripts":
    [
        {
            "matches":["<all_urls>"],
            "js": ["js/content-script.js"],
            "css": ["css/custom.css"],
            "run_at": "document_start"
        }
    ],
    "permissions":
    [
        "contextMenus",
        "tabs",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources":["js/inject.js"],
    "homepage_url":"https://www.baidu.com",
    "chrome_url_overrides":
    {
        "newTab":"newTab.html"
    },
    "content_security_policy":"script-src 'self' https://fundgz.1234567.com.cn/; object-src 'self'",
    "options_ui":
    {
        "page":"options.html",
        "chrome_style":true
    },
    "omnibox":{"keyword":"go"},
    "defaul_locale":"zh_CN",
    "devtools_page":"devtools.html"
}